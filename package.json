{
  "name": "vue-dict",
  "version": "0.0.0",
  "type": "module",
  "description": "vue3 dict manager",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./lib/index.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "build:type": "tsc -p ./tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.0",
    "ts-toolbelt": "^9.6.0",
    "tsup": "^8.0.1",
    "type-fest": "^4.9.0",
    "typescript": "^5.3.3",
    "vue": "^3.4.10"
  }
}
